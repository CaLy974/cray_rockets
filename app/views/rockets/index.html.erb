<div class="header">
</div>

<div class="container">
  <div class="row row-cols-1 row-cols-md-3 g-4">
    <% @rockets.each do |rocket| %>
      <div class="col">
        <div class="card">
          <div class="card-body">
            <h5 class="card-title"><%= rocket.name %></h5>
            <p class="card-text"><%= rocket.price %></p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>

<div class="footer">
</div>
